  EINHEIT_W   "K"
  RCVX   DISTRIBUTION X
  ST/X  1.5 2.5 3.5
  WERT  -50.0 -95.0 -140.0
END

KENNFELD revisedMap 6 2
  LANGNAME  "Revised map"
  FUNKTION  "MapFunction"
  DISPLAYNAME  MapDisplayname
  EINHEIT_X   "K"
  EINHEIT_Y   "m^2/s"
  EINHEIT_W   "kPa"
